{
  "name": "@hive/persisted-documents",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsx ../../../scripts/runify.ts",
    "dev": "tsup-node --config ../../../configs/tsup/dev.config.node.ts src/dev.ts",
    "postbuild": "copyfiles -f \"node_modules/bullmq/dist/esm/commands/*.lua\" dist && copyfiles -f \"node_modules/bullmq/dist/esm/commands/includes/*.lua\" dist/includes",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@hive/cdn-script": "workspace:*",
    "@hive/service-common": "workspace:*",
    "@sentry/node": "7.76.0",
    "@sinclair/typebox": "0.31.18",
    "@whatwg-node/fetch": "0.9.13",
    "bullmq": "4.12.7",
    "copyfiles": "2.4.1",
    "dotenv": "16.3.1",
    "ioredis": "5.3.2",
    "p-timeout": "6.1.2",
    "pino-pretty": "10.2.3",
    "slonik": "30.4.4",
    "tslib": "2.6.2",
    "zod": "3.22.4"
  }
}
